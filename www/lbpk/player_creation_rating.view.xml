<?xml version="1.0" encoding="UTF-8"?>
<resource name="player_creation_rating.view">
  <request method="GET" url="api/v1/player_creation_rating.view.xml" login_required="true">
    <param name="player_creation_id" type="integer" required="true"/>
    <param name="player_id" type="integer" required="false"/>
  </request>
  <response name="response" type="anchor">
    <element name="player_creation_rating" type="anchor">
      <attribute name="comments" type="string"/>
      <attribute name="rating" type="integer"/>
    </element>
  </response>
</resource>
