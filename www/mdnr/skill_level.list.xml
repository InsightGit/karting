<?xml version="1.0" encoding="UTF-8"?>
<resource name="skill_level.list">
  <request method="GET" url="api/v1/skill_level.list.xml"/>
  <response name="response" type="anchor">
    <element name="skill_levels" type="anchor">
      <attribute name="total" type="integer"/>
      <vector name="skill_level" type="anchor">
        <attribute name="id" type="integer"/>
        <attribute name="name" type="string"/>
        <attribute name="points" type="integer"/>
      </vector>
    </element>
  </response>
</resource>
