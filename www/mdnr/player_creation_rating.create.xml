<?xml version="1.0" encoding="UTF-8"?>
<resource name="player_creation_rating.create" login_required="true">
  <request method="POST" url="api/v1/player_creation_rating.create.xml">
    <param name="player_creation_rating[player_creation_id]" type="integer" minvalue="10000" required="true"/>
    <param name="player_creation_rating[rating]" type="integer" options="1,2,3,4,5" required="false"/>
    <param name="player_creation_rating[comments]" type="string"/>
  </request>
</resource>
